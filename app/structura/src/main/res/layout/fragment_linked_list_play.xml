<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.davisosa.structura.LinkedListPlayFragment">

    <!-- TODO: Update blank fragment layout -->
    <com.melnykov.fab.FloatingActionButton
        android:id="@+id/add"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|right"
        android:layout_margin="16dp"
        android:src="@drawable/ic_action_add"
        fab:fab_colorNormal="@color/add_primary"
        fab:fab_colorPressed="@color/add_primary_pressed"
        fab:fab_colorRipple="@color/add_ripple" />

    <com.melnykov.fab.FloatingActionButton
        android:id="@+id/sub"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|right"
        android:src="@drawable/ic_action_remove"
        fab:fab_colorNormal="@color/del_primary"
        fab:fab_colorPressed="@color/del_primary_pressed"
        fab:fab_colorRipple="@color/del_ripple"
        android:layout_marginRight="80dp"
        android:layout_marginBottom="16dp" />

</FrameLayout>
